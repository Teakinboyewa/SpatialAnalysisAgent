<?xml version='1.0' encoding='utf-8'?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd"><key id="d2" for="node" attr.name="description" attr.type="string"/>
<key id="d1" for="node" attr.name="data_path" attr.type="string"/>
<key id="d0" for="node" attr.name="node_type" attr.type="string"/>
<graph edgedefault="directed"><node id="input_DEM">
  <data key="d0">data</data>
  <data key="d1">D:/Data/DEM.tif</data>
  <data key="d2">Digital Elevation Model</data>
</node>
<node id="slope_raster">
  <data key="d0">data</data>
  <data key="d1"></data>
  <data key="d2">Slope raster derived from DEM</data>
</node>
<node id="aspect_raster">
  <data key="d0">data</data>
  <data key="d1"></data>
  <data key="d2">Aspect raster derived from DEM</data>
</node>
<node id="hillshade_raster">
  <data key="d0">data</data>
  <data key="d1"></data>
  <data key="d2">Hillshade raster derived from DEM</data>
</node>
<node id="filtered_raster">
  <data key="d0">data</data>
  <data key="d1"></data>
  <data key="d2">Raster satisfying conditions: slope between 0.3 and 0.5, aspect between 200 and 250, hillshade &lt; 350</data>
</node>
<node id="calculate_slope">
  <data key="d0">operation</data>
  <data key="d2">Calculate slope from DEM</data>
</node>
<node id="calculate_aspect">
  <data key="d0">operation</data>
  <data key="d2">Calculate aspect from DEM</data>
</node>
<node id="calculate_hillshade">
  <data key="d0">operation</data>
  <data key="d2">Calculate hillshade from DEM</data>
</node>
<node id="filter_raster">
  <data key="d0">operation</data>
  <data key="d2">Filter raster satisfying all conditions for slope, aspect, and hillshade</data>
</node>
<edge source="input_DEM" target="calculate_slope"/>
<edge source="input_DEM" target="calculate_aspect"/>
<edge source="input_DEM" target="calculate_hillshade"/>
<edge source="slope_raster" target="filter_raster"/>
<edge source="aspect_raster" target="filter_raster"/>
<edge source="hillshade_raster" target="filter_raster"/>
<edge source="calculate_slope" target="slope_raster"/>
<edge source="calculate_aspect" target="aspect_raster"/>
<edge source="calculate_hillshade" target="hillshade_raster"/>
<edge source="filter_raster" target="filtered_raster"/>
</graph></graphml>